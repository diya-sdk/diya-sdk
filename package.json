{
  "name": "diya-sdk",
  "version": "0.5.0",
  "description": "SDK for accessing Diya One services",
  "main": "src/diya-sdk.js",
  "scripts": {
    "build-debug": "browserify src/diya-sdk.js -d --s diya > dist/diya-sdk.js",
    "build-min": "browserify src/diya-sdk.js --s diya | uglifyjs -c > dist/diya-sdk.min.js",
    "build": "npm run build-debug && npm run build-min",
    "watch": "watchify src/diya-sdk.js -d --s diya -o dist/diya-sdk.js -v"
  },
  "author": "Sylvain Mah√© <sylv.mahe@gmail.com> (https://github.com/nilpotence)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^5.12.0",
    "uglify-js": "^2.4.15",
    "watchify": "^1.0.5"
  }
}
